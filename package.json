    "@hookform/resolvers": "^2.9.10",
    "@lancedb/vectordb-darwin-x64": "^0.2.3",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@types/needle": "^3.2.0",
    "@vercel/analytics": "^1.0.1",
    "ai": "^2.2.8",
    "archiver": "^5.3.2",
    "boring-avatars": "^1.7.0",
    "eventemitter3": "^5.0.1",
    "file-loader": "^6.2.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.12.18",
    "immer": "^10.0.2",
    "langchain": "^0.0.127",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "needle": "^3.2.0",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "nextjs-google-analytics": "^2.3.3",
    "nodemailer": "^6.9.3",
    "nookies": "^2.5.2",
    "nprogress": "^0.2.0",
    "openai": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.10.1",
    "superjson": "1.12.2",
    "timeago.js": "^4.0.2",
    "trpc-panel": "^1.3.4",
    "usehooks-ts": "^2.9.1",
    "vectordb": "^0.2.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.9"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.2.3",
    "@types/archiver": "^5.3.2",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
